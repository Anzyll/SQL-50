class Solution {
    public long maximumSubarraySum(int[] nums, int k) {
        Set<Integer> set = new HashSet();
        int start = 0;
        long curSum=0;
        long maxSum = 0;
        for(int end=0;end<nums.length;end++){
            while(set.contains(nums[end]) || set.size()==k){
                set.remove(nums[start]);
                curSum-=nums[start];
                start++;
            }
            curSum+=nums[end];
            set.add(nums[end]);
            if(set.size()==k){
                maxSum=Math.max(curSum,maxSum);
            }
        }
        return maxSum;
    }
}
